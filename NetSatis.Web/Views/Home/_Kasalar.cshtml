<div class="row">
    <div class="col-md-12">
        <div class="main-card mb-3 card">
            <div class="card-header">
               Kasa Bakiyesi 
                <div class="btn-actions-pane-right">

                </div>
            </div>
            <div class="table-responsive">
                <table class="align-middle mb-0 table table-borderless table-striped table-hover">
                    <thead>
                        <tr>
                            <th class="text-left">Kasa Kodu </th>
                            <th class="text-left">Kasa Adı</th>
                            <th class="text-left">Giriş Bakiyesi </th>
                            <th class="text-left">Çıkış Bakiyesi</th>
                            <th class="text-left"> Bakiye  </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var Kasa in Model)
                        {
                        <tr>
                            <td class="text-left text-muted">@Kasa.KasaKodu</td>
                            <td class="text-left text-muted">@Kasa.KasaAdi</td>
                            <td class="text-left text-muted">@Kasa.KasaGiris</td>
                            <td class="text-left text-muted">@Kasa.KasaCikis</td>
                            <td class="text-left text-muted">@Kasa.Bakiye</td>

                        </tr>
                        }

                    </tbody>
                </table>
            </div>
            <div class="d-block text-center card-footer">

            </div>
 

        </div>
    </div>
</div>